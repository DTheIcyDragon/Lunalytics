{
  "name": "lunalytics",
  "version": "0.8.6",
  "description": "Open source Node.js server/website monitoring tool",
  "private": true,
  "author": "KSJaay <ksjaay@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ksjaay/lunalytics.git"
  },
  "engines": {
    "node": ">= 22.x"
  },
  "scripts": {
    "dev": "concurrently \"npm run watch\" \"npm run server:watch\"",
    "watch": "vite --port 3000",
    "build": "vite build --config ./vite.config.js",
    "docker": "node ./scripts/setup.js && npm run start",
    "lint": "npm run lint:app && npm run lint:server",
    "lint:app": "oxlint app",
    "lint:server": "oxlint server",
    "test:app": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npm run build && cypress open",
    "test:db": "node ./test/shared/setupDatabase.js",
    "test:server": "npm run test:db && cross-env NODE_ENV=test node server/index.js",
    "migrate": "node ./scripts/migrate_manual.js",
    "dev:prepare": "husky",
    "preview": "vite preview",
    "reset:password": "node ./scripts/reset.js",
    "server:watch": "nodemon --inspect --delay 2 --watch server --watch shared ./server/index.js",
    "setup": "npm install && node ./scripts/setup.js && npm run build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.9.1",
    "classnames": "^2.5.1",
    "cookie": "^1.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.1.3",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "mongoose": "^8.13.2",
    "nanoid": "^5.1.5",
    "pg": "^8.14.1",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.0",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.2",
    "swapy": "^1.0.5",
    "ua-parser-js": "^2.0.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "@vitest/coverage-v8": "^3.1.1",
    "concurrently": "^9.1.2",
    "cypress": "^14.2.1",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "inquirer": "^12.5.2",
    "lint-staged": "^15.5.0",
    "node-mocks-http": "^1.16.2",
    "nodemon": "^3.1.9",
    "oxlint": "^0.15.15",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.86.3",
    "vite": "^6.2.5",
    "vite-plugin-compression2": "^1.3.3",
    "vitepress": "^1.6.3",
    "vitest": "^3.1.1"
  }
}
