{
  "name": "lunalytics",
  "version": "0.2.0",
  "description": "Open source Node.js server/website monitoring tool",
  "private": true,
  "author": "KSJaay <ksjaay@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ksjaay/lunalytics.git"
  },
  "scripts": {
    "dev": "concurrently \"yarn watch\" \"yarn server:watch\"",
    "watch": "vite --port 3000",
    "build": "vite build",
    "lint:app": "eslint app --ext js,jsx",
    "lint:server": "eslint server --ext js",
    "prepare": "husky install",
    "preview": "vite preview",
    "server:watch": "nodemon --delay 2 --watch server ./server/index.js"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.1",
    "chartjs-adapter-dayjs-3": "^1.2.3",
    "classnames": "^2.5.1",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.19.0",
    "sonner": "^1.3.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "sass": "^1.69.5",
    "vite": "^5.0.10"
  }
}
